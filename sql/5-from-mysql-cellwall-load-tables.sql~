--------------------------------------------------------------------------------------------------------------------------------- 
-- load sequence to stage
---------------------------------------------------------------------------------------------------------------------------------

drop table if exists cellwall.st_sequence;

create table cellwall.st_sequence 
(       
       source    varchar,       
       load_date date,       
       header    varchar,       
       sequence  varchar
);

truncate table cellwall.st_sequence;
-- select * from cellwall.st_sequence limit 100;

-- PHYPA ------------------------------------------------------------------------------------------------------------------------

alter table cellwall.st_sequence alter column source set default 'PHYPA';
alter table cellwall.st_sequence alter column load_date set default '20090418';

copy cellwall.st_sequence ( header, sequence ) from 'c:\\db-dumps\\source_downloaded-2009-04-10_fasta_jgi-phypa-v1-1_prot--.tab';
copy cellwall.st_sequence ( header, sequence ) from 'c:\\db-dumps\\source_downloaded-2009-04-10_fasta_jgi-phypa-v1-1_trans-.tab';
-- copy cellwall.st_sequence ( header, sequence ) from 'c:\\db-dumps\\source_downloaded-2009-04-10_gff--_jgi-phypa-v1-1_genes-.tab'; does not look like a sequence file

commit;

alter table cellwall.st_sequence alter column source set default null;
alter table cellwall.st_sequence alter column load_date set default null;

